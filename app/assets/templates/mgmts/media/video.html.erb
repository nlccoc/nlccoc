<style>
a {
  text-decoration: none !important;
}

p, li, a{
  font-size: 14px;
}

.pagination{
  padding: 30px 0;
}

.pagination ul{
  margin: 0;
  padding: 0;
  list-style-type: none;
}

.pagination a{
  display: inline-block;
  padding: 10px 18px;
  color: #222;
}

/* ONE */

.p1 a{
  width: 40px;
  height: 40px;
  line-height: 40px;
  padding: 0;
  text-align: center;
}

.p1 a.is-active{
  background-color: #2ecc71;
  border-radius: 100% !important;
  color: #fff;
}

.p9 a {
  width: 30px;
  height: 30px;
  line-height: 24px;
  padding: 0;
  text-align: center;
  margin: auto 5px;
}

.p9 a.is-active{
  border: 3px solid #2ecc71;
  border-radius: 100% !important;
}
.modal-content {
  position: relative;
  width: 90%;
  max-width: 400px;
  margin: 14em auto;
  background: #FFF;
  border: none;
  border-radius: 0;
  /*border-radius: .25em .25em .4em .4em;*/
  text-align: center;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
  -webkit-transform: translateY(-40px);
  -moz-transform: translateY(-40px);
  -ms-transform: translateY(-40px);
  -o-transform: translateY(-40px);
  transform: translateY(-40px);
  /* Force Hardware Acceleration in WebKit */
  -webkit-backface-visibility: hidden;
  -webkit-transition-property: -webkit-transform;
  -moz-transition-property: -moz-transform;
  transition-property: transform;
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}
.modal-content p {
  padding: 3em 1em;
}
.modal-content .cd-buttons,
.modal-content p {
  margin-bottom: 0px;
}


.modal-content .cd-buttons:after {
  content: "";
  display: table;
  clear: both;
}

.modal-content .cd-buttons {
  padding: 0px;
}
.modal-content .cd-buttons li {
  list-style: none;
  float: left;
  width: 50%;
}
.modal-content .cd-buttons a {
  display: block;
  height: 60px;
  line-height: 60px;
  text-transform: uppercase;
  color: #FFF;
  -webkit-transition: background-color 0.2s;
  -moz-transition: background-color 0.2s;
  transition: background-color 0.2s;
}
.modal-content .cd-buttons li:first-child a {
  background: #fc7169;
  /*border-radius: 0 0 0 .25em;*/
}
.no-touch .modal-content .cd-buttons li:first-child a:hover {
  background-color: #fc8982;
}
.modal-content .cd-buttons li:last-child a {
  background: #b6bece;
  /*border-radius: 0 0 .25em 0;*/
}
.no-touch .modal-content .cd-buttons li:last-child a:hover {
  background-color: #c5ccd8;
}
.modal-content .cd-popup-close {
  position: absolute;
  top: 8px;
  right: 8px;
  width: 30px;
  height: 30px;
}
.modal-content .cd-popup-close::before, .modal-content .cd-popup-close::after {
  content: '';
  position: absolute;
  top: 12px;
  width: 14px;
  height: 3px;
  background-color: #8f9cb5;
}
.modal-content .cd-popup-close::before {
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
  left: 8px;
}
.modal-content .cd-popup-close::after {
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  -o-transform: rotate(-45deg);
  transform: rotate(-45deg);
  right: 8px;
}
</style>
<!-- BEGIN PAGE BAR -->
<div class="page-bar">
  <ul class="page-breadcrumb">
    <li>
      <a href="index.html">Home</a>
      <i class="fa fa-circle"></i>
    </li>
    <li>
      <span>All Video</span>
    </li>
  </ul>
</div>
<!-- END PAGE BAR -->

<h1 class="page-title"> All Video
  <!--<small>All Events</small>-->
</h1>

<div class='row'>
  <div class='col-md-12'>
    <a class="btn green" ui-sref='mvideo-new'>New Video</a>
  </div>
</div>
<div class='row'>
  <div class='col-md-12'>
    <div class="pagination p1" ng-show='total_pages > 1'>
      <ul>
        <a href="#" ng-show='current_page != 1' ><li><</li></a>
        <a href='#' ui-sref='media-video({page: i})' ng-class="{'is-active': i==current_page}" ng-repeat="i in []| range: total_pages:1">{{i}}</a>
        <a href="#" ng-show='current_page != total_pages' ><li>></li></a>
      </ul>
    </div>
    
    <table id="maudios" class="table table-bordered table-striped">
      <thead>
        <tr>
          <th style="width:15%">Title</th>
          <th style="width:10%" >Date</th>
          <th style="width:30%" >Address</th>
          <th style="width:10%" >Location</th>
          <th style="width:30%">Description</th>
          <th style="width:15%"></th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat='video in mvideos'>
          <td style="width:15%"> {{video.title}}</td>
          <td style="width:10%"> {{video.date}}</td>
          <td style="width:30%"> {{video.path}}</td>
          <td style="width:10%"> {{video.location.name}}</td>
          <td style="width:30%"> {{video.desc}}</td>
          <td style="width:15%">
            <span class="text-muted"> <a  ui-sref='mvideo-edit({mvideoID: video.id})' href="#">
              <i class="fa fa-pencil" aria-hidden="true"></i></a>
            </span>  
            <span class="text-muted"> 
              <a ng-click='deleteMVideo(video);' href="#"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
            </span>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
