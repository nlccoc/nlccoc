<h1 class="page-title"> All Audio
  <!--<small>All Events</small>-->
</h1>

<div class='row'>
  <div class='col-md-12'>
    <a class="btn green" ui-sref='audio-new'>New Audio</a>
  </div>
</div>
<div class='row'>
  <div class='col-md-12'>
    <table id="maudios" class="table table-bordered table-striped">
      <thead>
        <tr>
          <th style="width:10%">Name</th>
          <th style="width:30%" >Desc</th>
          <th style="width:30%" >Path</th>
          <th style="width:10%" >Date</th>
          <th style="width:10%">Featured</th>
          <th style="width:10%"></th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat='maudio in maudios'>
          <td style="width:10%"> {{maudio.title}}</td>
          <td style="width:30%"> {{maudio.desc}}</td>
          <td style="width:30%"> {{maudio.path}}</td>
          <td style="width:10%"> {{maudio.date}}</td>
          <td style="width:10%"> {{maudio.featured}}</td>
          <td style="width:10%">
            <span class="text-muted"> <a  ui-sref='maudio-edit({maudioID: maudio.id})' href="#">
              <i class="fa fa-pencil" aria-hidden="true"></i></a>
            </span>  
            <span class="text-muted"> 
              <a ng-click='deleteMaudio(maudio);' href="#"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
            </span>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>