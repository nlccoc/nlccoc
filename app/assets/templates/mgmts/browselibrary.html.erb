<style>
  
.load-wrapp {
  float: left;
  width: 100px;
  height: 100px;
  margin: 0 10px 10px 0;
  padding: 34px 20px 20px;
  border-radius: 5px;
  text-align: center;
  background-color: #d8d8d8;
}

.load-wrapp p {
    padding: 0 0 20px;
}
p {
    margin: 0;
    padding: 10px 0;
    color: #777;
}

@keyframes loadingD {
    0 {transform: rotate(0deg);}
    50% {transform: rotate(180deg);}
    100% {transform: rotate(360deg);}
}

.load-4 .ring-1 {
  animation: loadingD 1.5s .3s cubic-bezier(.17,.37,.43,.67) infinite;
  
}

.ring-1 {
    width: 10px;
    height: 10px;
    margin: 0 auto;
    padding: 10px;
    border: 7px dashed #4b9cdb;
    border-radius: 100% !important;
}
</style>
<div style='display: inline-block'>
<p>Browse Libraries</p>
<div>
	<library-list ng-repeat='library in libraries' src='{{library.attachment.thumb.url}}'></library-list>
</div>
</div>
