<style>

a {
  text-decoration: none !important;
}

p, li, a{
  font-size: 14px;
}

.pagination{
  padding: 30px 0;
}

.pagination ul{
  margin: 0;
  padding: 0;
  list-style-type: none;
}

.pagination a{
  display: inline-block;
  padding: 10px 18px;
  color: #222;
}

/* ONE */

.p1 a{
  width: 40px;
  height: 40px;
  line-height: 40px;
  padding: 0;
  text-align: center;
}

.p1 a.is-active{
  background-color: #2ecc71;
  border-radius: 100% !important;
  color: #fff;
}

.p9 a {
  width: 30px;
  height: 30px;
  line-height: 24px;
  padding: 0;
  text-align: center;
  margin: auto 5px;
}

.p9 a.is-active{
  border: 3px solid #2ecc71;
  border-radius: 100% !important;
}

</style>
<!-- BEGIN PAGE HEADER-->
<div class="page-bar">
  <ul class="page-breadcrumb">
    <li>
        <i class="fa fa-home"></i>
        <a ui-sref="dashboard">Home</a>
        <i class="fa fa-angle-right"></i>
    </li>
    <li>
        <a ui-sref="datatablesmanaged">Logs</a>
    </li>
    <li>
        <a ui-sref="datatablesmanaged">All Logs</a>
    </li>
  </ul>
</div>
<h3 class="page-title"> Logs
  <small>All Logs</small>
</h3>
<!-- END PAGE HEADER-->
<!-- BEGIN MAIN CONTENT -->
<div class="row">
  <div class="col-md-12">
    <!-- BEGIN EXAMPLE TABLE PORTLET-->
    <div class="pagination p9" ng-show='total_pages > 1'>
      <ul>
        <a href="#" ng-show='current_page != 1' ><li><</li></a>
        <a href='#' ui-sref='logs-all({page: i})' ng-class="{'is-active': i==current_page}" ng-repeat="i in []| range: total_pages:1">{{i}}</a>
        <a href="#" ng-show='current_page != total_pages' ><li>></li></a>
      </ul>
    </div>
    <div class="portlet-body">
      <table class="table table-striped table-bordered table-hover table-checkable order-column" id="sample_1">
            <thead>
                <tr>
                    <th>
                        <label class="mt-checkbox mt-checkbox-outline mt-checkbox-single">
                            <input type="checkbox" class="group-checkable" data-set="#sample_1 .checkboxes" />
                            <span></span>
                        </label>
                    </th>
                    <th> Datetime </th>
                    <th> Log Message </th>
                    <th> Type </th>
                  </tr>
            </thead>
            <tbody>
              <tr class="odd gradeX" ng-repeat='log in logs' ng-class="logClassed(log)">
                  <td>
                      <label class="mt-checkbox mt-checkbox-outline mt-checkbox-single">
                          <input type="checkbox" class="checkboxes" value="1" />
                          <span></span>
                      </label>
                  </td>
                  <td>{{formatDateTime(log.datetime)}}</td>
                  <td><span ng-bind-html="html_safe(log.text)"></span></td>
                  <td>{{log.logtype.name}}</td>
              </tr>
                
            </tbody>
        </table>
    </div>
  </div>
    <!-- END EXAMPLE TABLE PORTLET-->
  </div>
</div>