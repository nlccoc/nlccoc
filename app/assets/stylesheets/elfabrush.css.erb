@font-face {
	font-family: 'elfa-brush';
	src:url('<%= asset_path("elfa-brush.eot") %>');
	src:url('<%= asset_path("elfa-brush.eot") %>') format('embedded-opentype'),
		url('<%= asset_path("elfa-brush.woff") %>') format('woff'),
		url('<%= asset_path("elfa-brush.ttf") %>') format('truetype'),
		url('<%= asset_path("elfa-brush.svg") %>') format('svg');
	font-weight: normal;
	font-style: normal;
}