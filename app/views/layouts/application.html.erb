<body>
  <% if content_for?(:laverne_navbar) %>
    <!--To show the La Verne page navbar-->
    <%= yield(:laverne_navbar) %>
  <% else %>
    <!--Use default navbar-->
    <%= yield(:default_navbar) %>
  <% end %>
  
  <!-- Loader Spinner -->
  <div id="loader-wrapper">
    <div id="spinner"></div>
  </div>
  <!-- End of Loader Spinner -->
  
  <!-- Yield main controller view -> views/main -->
  <%= yield %>
  <!-- End of main controller view -->
  
  <% if content_for?(:laverne_navbar) %>
    <%= render 'shared/main/laverne_footer'%>
  <% else %>
    <%= render 'shared/main/footer'%>
  <% end %>
  <%= render 'shared/main/quicknav' %>

  <!-- Go To Top button -->
  <a href='#top' class='inner-link'><div id="gotoTop" class="icon-angle-up hide"></div></a>
  <%= javascript_include_tag 'parallax', 'data-turbolinks-eval': true %>
  <!-- End of Go To Top button -->

</body>
</html>
