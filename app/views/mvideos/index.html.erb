<p id="notice"><%= notice %></p>

<h1>Listing Mvideos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th>Path</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mvideos.each do |mvideo| %>
      <tr>
        <td><%= mvideo.title %></td>
        <td><%= mvideo.date %></td>
        <td><%= mvideo.path %></td>
        <td><%= mvideo.desc %></td>
        <td><%= link_to 'Show', mvideo %></td>
        <td><%= link_to 'Edit', edit_mvideo_path(mvideo) %></td>
        <td><%= link_to 'Destroy', mvideo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mvideo', new_mvideo_path %>
