<div class="main-container">
	<section class="pt120 pb120 image-bg overlay parallax">
    <div class="background-image-holder">
      <img alt="Background" class="background-image" src="<%= asset_path('giveheart.jpg') %>">
    </div>
    <div class="container">
      <div class="row v-align-children">
        <div class="col-sm-8 mb-xs-80">
          <h2 class="mb16">Tithing Is More Than Giving
            <br class="hidden-sm"> </h2>
          <h6 class="uppercase mb32">The Bible says that tithing is a reminder that God is the supplier of everything. It teaches us to always put God first in our lives.</h6>
          <a class="btn btn-filled btn-white btn-lg mb0" href="#">十 一 奉 獻</a>
        </div>
        <div class="col-sm-4 text-center text-left-xs">
					<div class="modal-container">
						<div class="play-button btn-modal large inline"></div>
						<div class="foundry_modal no-bg">
					    <iframe data-provider="vimeo" data-video-id="25737856" data-autoplay="1" allowfullscreen="allowfullscreen" src=""></iframe>
						</div>
					</div>
        </div>
      </div>
      <div class="row v-align-children">
      <div class='col-sm-12 col-md-12'>
        <div class="align-bottom text-center hidden-xs">
          <a class='inner-link one-page-arrow' href='#build'><i class="icon-angle-down infinite animated fadeInDown"></i></a>
        </div>
        </div>
    </div>
    </div>
  </section>
  <section class="image-bg bg-dark parallax overlay pt120 pb120" id='build'>
    <div class="background-image-holder">
      <img alt="Background Image" class="background-image" src="<%= asset_path('project-single-6.jpg') %>" />
    </div>
    <div class="container">
      <div class="row">
        <div class="col-sm-12 text-right" style='padding-right:15%;'>
          <h2 class="uppercase mb8">建 堂</h2>
          <p class="lead mb40">建 堂 小 標</p>
          <%= link_to build_path(:locale => I18n.locale), :class => 'btn btn-lg btn-white mb0' do %>奉 獻 建 堂<% end %>
        </div>
      </div>
      <!--end of row-->
    </div>
    <!--end of container-->
  </section>
  <section class="image-bg bg-dark parallax overlay pt120 pb120">
    <div class="background-image-holder">
        <img alt="Background Image" class="background-image" src="<%= asset_path('home13.jpg') %>" />
    </div>
    <div class="container">
        <div class="row">
            <div class="col-sm-12 text-left" style='padding-left:15%;'>
                <h2 class="uppercase mb8">宣 教</h2>
                <p class="lead mb40">宣 教 小 標</p>
                <%= link_to mission_path(:locale => I18n.locale), :class => 'btn btn-lg btn-white mb0' do %>
                奉 獻 宣 教
                <% end %>
            </div>
        </div>
        <!--end of row-->
    </div>
    <!--end of container-->
  </section>
  <section class="image-bg bg-dark parallax overlay pt120 pb120">
    <div class="background-image-holder">
        <img alt="Background Image" class="background-image" src="<%= asset_path('resto1.jpg') %>" />
    </div>
    <div class="container">
        <div class="row">
            <div class="col-sm-12 text-right" style='padding-right:15%;'>
                <h2 class="uppercase mb8">愛 心 奉 獻</h2>
                <p class="lead mb40">愛 心 奉 獻 小 標</p>
                <a class="btn btn-lg btn-white mb0" href="#">愛 心 奉 獻</a>
            </div>
        </div>
        <!--end of row-->
    </div>
    <!--end of container-->
  </section>
</div>